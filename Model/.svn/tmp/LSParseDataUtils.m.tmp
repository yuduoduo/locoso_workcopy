{
        
    LSDataProduct* product = [[[LSDataProduct alloc] init] autorelease];
    
    product.producid = [[aJSONData valueForKey:LSJSONKEY_ProductId] longValue];
    product.productname = [aJSONData valueForKey:LSJSONKEY_ProductName];

    
    return product;

